pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Checkout code from Git repository
                git 'https://github.com/javaparser/javaparser-maven-sample'
                git credentialsId: '21ee699d-83b8-43f5-bc43-418b1505dbd9', url: 'https://github.com/jaswanthjangam99/javaparser-maven-sample'
            }
        }
stage('Checkout1') {
            steps {
                // Checkout code from Git repository
                git 'https://github.com/javaparser/javaparser-maven-sample'
                git credentialsId: '21ee699d-83b8-43f5-bc43-418b1505dbd9', url: 'https://github.com/jaswanthjangam99/javaparser-maven-sample'
            }
        }
        
       
        
       
        
       
    }
    
    post {
        always {
            // Clean up workspace
            cleanWs()
        }
        
        success {
            echo 'Pipeline successful!'
        }
        
        failure {
            echo 'Pipeline failed!'
        }
    }
}
